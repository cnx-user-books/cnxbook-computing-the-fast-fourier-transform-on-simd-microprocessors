                        <m:mo>=</m:mo>
                        <m:mn>0</m:mn>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>N</m:mi>
                        <m:mo>/</m:mo>
                        <m:mn>8</m:mn>
                        <m:mo>-</m:mo>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:munderover>
                    <m:mrow>
                      <m:msubsup>
                        <m:mi>ω</m:mi>
                        <m:mrow>
                          <m:mi>N</m:mi>
                          <m:mo>/</m:mo>
                          <m:mn>8</m:mn>
                        </m:mrow>
                        <m:mrow>
                          <m:msub>
                            <m:mi>n</m:mi>
                            <m:mn>8</m:mn>
                          </m:msub>
                          <m:mi>k</m:mi>
                        </m:mrow>
                      </m:msubsup>
                      <m:mspace width="4.pt"/>
                      <m:msub>
                        <m:mi>x</m:mi>
                        <m:mrow>
                          <m:mn>8</m:mn>
                          <m:msub>
                            <m:mi>n</m:mi>
                            <m:mn>8</m:mn>
                          </m:msub>
                          <m:mo>+</m:mo>
                          <m:mn>2</m:mn>
                        </m:mrow>
                      </m:msub>
                    </m:mrow>
                    <m:mo>+</m:mo>
                    <m:msubsup>
                      <m:mi>ω</m:mi>
                      <m:mrow>
                        <m:mi>N</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mo>-</m:mo>
                        <m:mn>2</m:mn>
                        <m:mi>k</m:mi>
                      </m:mrow>
                    </m:msubsup>
                    <m:munderover>
                      <m:mo>∑</m:mo>
                      <m:mrow>
                        <m:msub>
                          <m:mi>n</m:mi>
                          <m:mn>8</m:mn>
                        </m:msub>
                        <m:mo>=</m:mo>
                        <m:mn>0</m:mn>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>N</m:mi>
                        <m:mo>/</m:mo>
                        <m:mn>8</m:mn>
                        <m:mo>-</m:mo>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:munderover>
                    <m:mrow>
                      <m:msubsup>
                        <m:mi>ω</m:mi>
                        <m:mrow>
                          <m:mi>N</m:mi>
                          <m:mo>/</m:mo>
                          <m:mn>8</m:mn>
                        </m:mrow>
                        <m:mrow>
                          <m:msub>
                            <m:mi>n</m:mi>
                            <m:mn>8</m:mn>
                          </m:msub>
                          <m:mi>k</m:mi>
                        </m:mrow>
                      </m:msubsup>
                      <m:mspace width="4.pt"/>
                      <m:msub>
                        <m:mi>x</m:mi>
                        <m:mrow>
                          <m:mn>8</m:mn>
                          <m:msub>
                            <m:mi>n</m:mi>
                            <m:mn>8</m:mn>
                          </m:msub>
                          <m:mo>-</m:mo>
                          <m:mn>2</m:mn>
                        </m:mrow>
                      </m:msub>
                    </m:mrow>
                  </m:mrow>
                </m:mstyle>
              </m:mtd>
            </m:mtr>
            <m:mtr>
              <m:mtd/>
              <m:mtd>
                <m:mo>+</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:mstyle scriptlevel="0" displaystyle="true">
                  <m:mrow>
                    <m:msubsup>
                      <m:mi>ω</m:mi>
                      <m:mrow>
                        <m:mi>N</m:mi>
                      </m:mrow>
                      <m:mi>k</m:mi>
                    </m:msubsup>
                    <m:munderover>
                      <m:mo>∑</m:mo>
                      <m:mrow>
                        <m:msub>
                          <m:mi>n</m:mi>
                          <m:mn>4</m:mn>
                        </m:msub>
                        <m:mo>=</m:mo>
                        <m:mn>0</m:mn>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>N</m:mi>
                        <m:mo>/</m:mo>
                        <m:mn>4</m:mn>
                        <m:mo>-</m:mo>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:munderover>
                    <m:mrow>
                      <m:msubsup>
                        <m:mi>ω</m:mi>
                        <m:mrow>
                          <m:mi>N</m:mi>
                          <m:mo>/</m:mo>
                          <m:mn>4</m:mn>
                        </m:mrow>
                        <m:mrow>
                          <m:msub>
                            <m:mi>n</m:mi>
                            <m:mn>4</m:mn>
                          </m:msub>
                          <m:mi>k</m:mi>
                        </m:mrow>
                      </m:msubsup>
                      <m:mspace width="4.pt"/>
                      <m:msub>
                        <m:mi>x</m:mi>
                        <m:mrow>
                          <m:mn>4</m:mn>
                          <m:msub>
                            <m:mi>n</m:mi>
                            <m:mn>4</m:mn>
                          </m:msub>
                          <m:mo>+</m:mo>
                          <m:mn>1</m:mn>
                        </m:mrow>
                      </m:msub>
                    </m:mrow>
                    <m:mo>+</m:mo>
                    <m:msubsup>
                      <m:mi>ω</m:mi>
                      <m:mrow>
                        <m:mi>N</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mo>-</m:mo>
                        <m:mi>k</m:mi>
                      </m:mrow>
                    </m:msubsup>
                    <m:munderover>
                      <m:mo>∑</m:mo>
                      <m:mrow>
                        <m:msub>
                          <m:mi>n</m:mi>
                          <m:mn>4</m:mn>
                        </m:msub>
                        <m:mo>=</m:mo>
                        <m:mn>0</m:mn>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>N</m:mi>
                        <m:mo>/</m:mo>
                        <m:mn>4</m:mn>
                        <m:mo>-</m:mo>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:munderover>
                    <m:mrow>
                      <m:msubsup>
                        <m:mi>ω</m:mi>
                        <m:mrow>
                          <m:mi>N</m:mi>
                          <m:mo>/</m:mo>
                          <m:mn>4</m:mn>
                        </m:mrow>
                        <m:mrow>
                          <m:msub>
                            <m:mi>n</m:mi>
                            <m:mn>4</m:mn>
                          </m:msub>
                          <m:mi>k</m:mi>
                        </m:mrow>
                      </m:msubsup>
                      <m:mspace width="4.pt"/>
                      <m:msub>
                        <m:mi>x</m:mi>
                        <m:mrow>
                          <m:mn>4</m:mn>
                          <m:msub>
                            <m:mi>n</m:mi>
                            <m:mn>4</m:mn>
                          </m:msub>
                          <m:mo>-</m:mo>
                          <m:mn>1</m:mn>
                        </m:mrow>
                      </m:msub>
                    </m:mrow>
                  </m:mrow>
                </m:mstyle>
              </m:mtd>
            </m:mtr>
            <m:mtr>
              <m:mtd columnalign="right">
                <m:mrow/>
              </m:mtd>
            </m:mtr>
          </m:mtable>
        </m:math>
      </equation>
      <para id="id299783">where <m:math overflow="scroll"><m:mrow><m:msub><m:mi>x</m:mi><m:mrow><m:mo>-</m:mo><m:mi>n</m:mi></m:mrow></m:msub><m:mo>=</m:mo><m:msub><m:mi>x</m:mi><m:mrow><m:mi>N</m:mi><m:mo>-</m:mo><m:mi>n</m:mi></m:mrow></m:msub></m:mrow></m:math>. Note that the sums over <m:math overflow="scroll"><m:msub><m:mi>x</m:mi><m:mrow><m:mn>8</m:mn><m:msub><m:mi>n</m:mi><m:mn>8</m:mn></m:msub><m:mo>+</m:mo><m:mn>2</m:mn></m:mrow></m:msub></m:math> and
<m:math overflow="scroll"><m:msub><m:mi>x</m:mi><m:mrow><m:mn>8</m:mn><m:msub><m:mi>n</m:mi><m:mn>8</m:mn></m:msub><m:mo>-</m:mo><m:mn>2</m:mn></m:mrow></m:msub></m:math> are multiplied by constants that are now conjugate pairs, as are
the sums over <m:math overflow="scroll"><m:msub><m:mi>x</m:mi><m:mrow><m:mn>4</m:mn><m:msub><m:mi>n</m:mi><m:mn>4</m:mn></m:msub><m:mo>+</m:mo><m:mn>1</m:mn></m:mrow></m:msub></m:math> and <m:math overflow="scroll"><m:msub><m:mi>x</m:mi><m:mrow><m:mn>4</m:mn><m:msub><m:mi>n</m:mi><m:mn>4</m:mn></m:msub><m:mo>-</m:mo><m:mn>1</m:mn></m:mrow></m:msub></m:math>.</para>
      <para id="id299932">The sum over <m:math overflow="scroll"><m:msub><m:mi>x</m:mi><m:mrow><m:mn>4</m:mn><m:msub><m:mi>n</m:mi><m:mn>4</m:mn></m:msub></m:mrow></m:msub></m:math> is now substituted with <m:math overflow="scroll"><m:msub><m:mi>U</m:mi><m:mi>k</m:mi></m:msub></m:math> (where <m:math overflow="scroll"><m:mrow><m:mi>k</m:mi><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mo>⋯</m:mo><m:mo>,</m:mo><m:mi>N</m:mi><m:mo>/</m:mo><m:mn>4</m:mn><m:mo>-</m:mo><m:mn>1</m:mn></m:mrow></m:math>), while the sums over <m:math overflow="scroll"><m:msub><m:mi>x</m:mi><m:mrow><m:mn>8</m:mn><m:msub><m:mi>n</m:mi><m:mn>8</m:mn></m:msub><m:mo>+</m:mo><m:mn>2</m:mn></m:mrow></m:msub></m:math> and <m:math overflow="scroll"><m:msub><m:mi>x</m:mi><m:mrow><m:mn>8</m:mn><m:msub><m:mi>n</m:mi><m:mi>n</m:mi></m:msub><m:mo>-</m:mo><m:mn>2</m:mn></m:mrow></m:msub></m:math> are
respectively substituted with <m:math overflow="scroll"><m:msub><m:mi>Y</m:mi><m:mi>k</m:mi></m:msub></m:math> and <m:math overflow="scroll"><m:msubsup><m:mi>Y</m:mi><m:mi>k</m:mi><m:mo>'</m:mo></m:msubsup></m:math> (where <m:math overflow="scroll"><m:mrow><m:mi>k</m:mi><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mo>⋯</m:mo><m:mo>,</m:mo><m:mi>N</m:mi><m:mo>/</m:mo><m:mn>8</m:mn><m:mo>-</m:mo><m:mn>1</m:mn></m:mrow></m:math>) and the sums over <m:math overflow="scroll"><m:msub><m:mi>x</m:mi><m:mrow><m:mn>4</m:mn><m:msub><m:mi>n</m:mi><m:mn>4</m:mn></m:msub><m:mo>+</m:mo><m:mn>1</m:mn></m:mrow></m:msub></m:math> and <m:math overflow="scroll"><m:msub><m:mi>x</m:mi><m:mrow><m:mn>4</m:mn><m:msub><m:mi>n</m:mi><m:mn>4</m:mn></m:msub><m:mo>-</m:mo><m:mn>1</m:mn></m:mrow></m:msub></m:math> respectively
substituted with <m:math overflow="scroll"><m:msub><m:mi>Z</m:mi><m:mi>k</m:mi></m:msub></m:math> and <m:math overflow="scroll"><m:msubsup><m:mi>Z</m:mi><m:mi>k</m:mi><m:mo>'</m:mo></m:msubsup></m:math> (where <m:math overflow="scroll"><m:mrow><m:mi>k</m:mi><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mo>⋯</m:mo><m:mo>,</m:mo><m:mi>N</m:mi><m:mo>/</m:mo><m:mn>4</m:mn><m:mo>-</m:mo><m:mn>1</m:mn></m:mrow></m:math>), simplifying <link target-id="uid23"/> thus:</para>
      <equation id="uid24">
        <m:math overflow="scroll" mode="display">
          <m:mrow>
            <m:msub>
              <m:mi>X</m:mi>
              <m:mi>k</m:mi>
            </m:msub>
            <m:mo>=</m:mo>
            <m:msub>
              <m:mi>U</m:mi>
              <m:mi>k</m:mi>
            </m:msub>
            <m:mo>+</m:mo>
            <m:msubsup>
              <m:mi>ω</m:mi>
              <m:mrow>
                <m:mi>N</m:mi>
              </m:mrow>
              <m:mrow>
                <m:mn>2</m:mn>
                <m:mi>k</m:mi>
              </m:mrow>
            </m:msubsup>
            <m:msub>
              <m:mi>Y</m:mi>
              <m:mi>k</m:mi>
            </m:msub>
            <m:mo>+</m:mo>
            <m:msubsup>
              <m:mi>ω</m:mi>
              <m:mi>N</m:mi>
              <m:mrow>
                <m:mo>-</m:mo>
                <m:mn>2</m:mn>
                <m:mi>k</m:mi>
              </m:mrow>
            </m:msubsup>
            <m:msubsup>
              <m:mi>Y</m:mi>
              <m:mi>k</m:mi>
              <m:mo>'</m:mo>
            </m:msubsup>
            <m:mo>+</m:mo>
            <m:msubsup>
              <m:mi>ω</m:mi>
              <m:mrow>
                <m:mi>N</m:mi>
              </m:mrow>
              <m:mi>k</m:mi>
            </m:msubsup>
            <m:msub>
              <m:mi>Z</m:mi>
              <m:mi>k</m:mi>
            </m:msub>
            <m:mo>+</m:mo>
            <m:msubsup>
              <m:mi>ω</m:mi>
              <m:mrow>
                <m:mi>N</m:mi>
              </m:mrow>
              <m:mrow>
                <m:mo>-</m:mo>
                <m:mi>k</m:mi>
              </m:mrow>
            </m:msubsup>
            <m:msubsup>
              <m:mi>Z</m:mi>
              <m:mi>k</m:mi>
              <m:mo>'</m:mo>
            </m:msubsup>
          </m:mrow>
        </m:math>
      </equation>
      <para id="id300377">As with earlier examples, computation is factored out of <link target-id="uid24"/> by exploiting periodicity in the
sub-transforms and symmetries in the twiddle factors. <link target-id="uid24"/> is first expressed as a parametric equation of eight parts:</para>
      <equation id="uid25">
        <m:math overflow="scroll" mode="display">
          <m:mtable displaystyle="true">
            <m:mtr>
              <m:mtd columnalign="right">
                <m:mrow>
                  <m:msub>
                    <m:mi>X</m:mi>
                    <m:mrow>
                      <m:mi>k</m:mi>
                      <m:mo>+</m:mo>
                      <m:mi>p</m:mi>
                      <m:mi>N</m:mi>
                    </m:mrow>
                  </m:msub>
                  <m:mo>=</m:mo>
                  <m:msub>
                    <m:mi>U</m:mi>
                    <m:mrow>
                      <m:mi>k</m:mi>
                      <m:mo>+</m:mo>
                      <m:mi>p</m:mi>
                      <m:mi>N</m:mi>
                    </m:mrow>
                  </m:msub>
                  <m:mo>+</m:mo>
                  <m:msubsup>
                    <m:mi>ω</m:mi>
                    <m:mi>N</m:mi>
                    <m:mrow>
                      <m:mn>2</m:mn>
                      <m:mo>(</m:mo>
                      <m:mi>k</m:mi>
                      <m:mo>+</m:mo>
                      <m:mi>p</m:mi>
                      <m:mi>N</m:mi>
                      <m:mo>)</m:mo>
                    </m:mrow>
                  </m:msubsup>
                  <m:msub>
                    <m:mi>Y</m:mi>
                    <m:mrow>
                      <m:mi>k</m:mi>
                      <m:mo>+</m:mo>
                      <m:mi>p</m:mi>
                      <m:mi>N</m:mi>
                    </m:mrow>
                  </m:msub>
                  <m:mo>+</m:mo>
                  <m:msubsup>
                    <m:mi>ω</m:mi>
                    <m:mi>N</m:mi>
                    <m:mrow>
                      <m:mo>-</m:mo>
                      <m:mn>2</m:mn>
                      <m:mo>(</m:mo>
                      <m:mi>k</m:mi>
                      <m:mo>+</m:mo>
                      <m:mi>p</m:mi>
                      <m:mi>N</m:mi>
                      <m:mo>)</m:mo>
                    </m:mrow>
                  </m:msubsup>
                  <m:msubsup>
                    <m:mi>Y</m:mi>
                    <m:mrow>
                      <m:mi>k</m:mi>
                      <m:mo>+</m:mo>
                      <m:mi>p</m:mi>
                      <m:mi>N</m:mi>
                    </m:mrow>
                    <m:mo>'</m:mo>
                  </m:msubsup>
                  <m:mo>+</m:mo>
                  <m:msubsup>
                    <m:mi>ω</m:mi>
                    <m:mi>N</m:mi>
                    <m:mrow>
                      <m:mi>k</m:mi>
                      <m:mo>+</m:mo>
                      <m:mi>p</m:mi>
                      <m:mi>N</m:mi>
                    </m:mrow>
                  </m:msubsup>
                  <m:msub>
                    <m:mi>Z</m:mi>
                    <m:mrow>
                      <m:mi>k</m:mi>
                      <m:mo>+</m:mo>
                      <m:mi>p</m:mi>
                      <m:mi>N</m:mi>
                    </m:mrow>
                  </m:msub>
                  <m:mo>+</m:mo>
                  <m:msubsup>
                    <m:mi>ω</m:mi>
                    <m:mi>N</m:mi>
                    <m:mrow>
                      <m:mo>-</m:mo>
                      <m:mo>(</m:mo>
                      <m:mi>k</m:mi>
                      <m:mo>+</m:mo>
                      <m:mi>p</m:mi>
                      <m:mi>N</m:mi>
                      <m:mo>)</m:mo>
                    </m:mrow>
                  </m:msubsup>
                  <m:msubsup>
                    <m:mi>Z</m:mi>
                    <m:mrow>
                      <m:mi>k</m:mi>
                      <m:mo>+</m:mo>
                      <m:mi>p</m:mi>
                      <m:mi>N</m:mi>
                    </m:mrow>
                    <m:mo>'</m:mo>
                  </m:msubsup>
                </m:mrow>
              </m:mtd>
            </m:mtr>
          </m:mtable>
        </m:math>
      </equation>
      <para id="id300615">where <m:math overflow="scroll"><m:mrow><m:mi>k</m:mi><m:mo>=</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mo>⋯</m:mo><m:mo>,</m:mo><m:mi>N</m:mi><m:mo>/</m:mo><m:mn>8</m:mn><m:mo>-</m:mo><m:mn>1</m:mn></m:mrow></m:math> and <m:math overflow="scroll"><m:mrow><m:mo>∀</m:mo><m:mi>p</m:mi><m:mo>∈</m:mo><m:mfenced separators="" open="{" close="}"><m:mrow><m:mn>0</m:mn><m:mo>,</m:mo><m:mfrac><m:mn>1</m:mn><m:mn>2</m:mn></m:mfrac><m:mo>,</m:mo><m:mfrac><m:mn>1</m:mn><m:mn>4</m:mn></m:mfrac><m:mo>,</m:mo><m:mfrac><m:mn>3</m:mn><m:mn>4</m:mn></m:mfrac><m:mo>,</m:mo><m:mfrac><m:mn>1</m:mn><m:mn>8</m:mn></m:mfrac><m:mo>,</m:mo><m:mfrac><m:mn>3</m:mn><m:mn>8</m:mn></m:mfrac><m:mo>,</m:mo><m:mfrac><m:mn>5</m:mn><m:mn>8</m:mn></m:mfrac><m:mo>,</m:mo><m:mfrac><m:mn>7</m:mn><m:mn>8</m:mn></m:mfrac></m:mrow></m:mfenced></m:mrow></m:math>. By exploiting periodicity in the sub-transforms:</para>
      <equation id="uid26">
        <m:math overflow="scroll" mode="display">
          <m:mtable>
            <m:mtr>
              <m:mtd columnalign="left">
                <m:msub>
                  <m:mi>U</m:mi>
                  <m:mi>k</m:mi>
                </m:msub>
              </m:mtd>
              <m:mtd>
                <m:mo>=</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:msub>
                  <m:mi>U</m:mi>
                  <m:mrow>
                    <m:mi>k</m:mi>
                    <m:mo>+</m:mo>
                    <m:mi>N</m:mi>
                    <m:mo>/</m:mo>
                    <m:mn>4</m:mn>
                  </m:mrow>
                </m:msub>
              </m:mtd>
            </m:mtr>
            <m:mtr>
              <m:mtd columnalign="left">
                <m:msub>
                  <m:mi>Y</m:mi>
                  <m:mi>k</m:mi>
                </m:msub>
              </m:mtd>
              <m:mtd>
                <m:mo>=</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:msub>
                  <m:mi>Y</m:mi>
                  <m:mrow>
                    <m:mi>k</m:mi>
                    <m:mo>+</m:mo>
                    <m:mi>N</m:mi>
                    <m:mo>/</m:mo>
                    <m:mn>8</m:mn>
                  </m:mrow>
                </m:msub>
              </m:mtd>
            </m:mtr>
            <m:mtr>
              <m:mtd columnalign="left">
                <m:msubsup>
                  <m:mi>Y</m:mi>
                  <m:mi>k</m:mi>
                  <m:mo>'</m:mo>
                </m:msubsup>
              </m:mtd>
              <m:mtd>
                <m:mo>=</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:msubsup>
                  <m:mi>Y</m:mi>
                  <m:mrow>
