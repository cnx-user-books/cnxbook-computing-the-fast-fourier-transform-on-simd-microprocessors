                    </m:msubsup>
                    <m:msubsup>
                      <m:mi>Y</m:mi>
                      <m:mi>k</m:mi>
                      <m:mo>'</m:mo>
                    </m:msubsup>
                    <m:mo>)</m:mo>
                  </m:mrow>
                  <m:msub>
                    <m:mi>ϵ</m:mi>
                    <m:mrow>
                      <m:mi>N</m:mi>
                      <m:mo>,</m:mo>
                      <m:mi>k</m:mi>
                    </m:mrow>
                  </m:msub>
                  <m:mo>+</m:mo>
                  <m:mi>i</m:mi>
                  <m:mrow>
