                      </m:msubsup>
                      <m:mspace width="4.pt"/>
                      <m:msub>
                        <m:mi>x</m:mi>
                        <m:mrow>
                          <m:mn>4</m:mn>
                          <m:msub>
                            <m:mi>n</m:mi>
                            <m:mn>4</m:mn>
                          </m:msub>
                        </m:mrow>
                      </m:msub>
                    </m:mrow>
                  </m:mrow>
                </m:mstyle>
              </m:mtd>
              <m:mtd>
                <m:mo>+</m:mo>
              </m:mtd>
              <m:mtd columnalign="left">
                <m:mstyle scriptlevel="0" displaystyle="true">
                  <m:mrow>
                    <m:msubsup>
                      <m:mi>ω</m:mi>
                      <m:mrow>
                        <m:mi>N</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mn>2</m:mn>
                        <m:mi>k</m:mi>
                      </m:mrow>
                    </m:msubsup>
                    <m:munderover>
                      <m:mo>∑</m:mo>
                      <m:mrow>
                        <m:msub>
                          <m:mi>n</m:mi>
